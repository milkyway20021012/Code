{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build (Make debug)",
      "type": "shell",
      "command": "make debug",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    },
    {
      "label": "Clean",
      "type": "shell",
      "command": "make clean",
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}

